{
    "manifest_version": 2,
    "name": "Tool Tài Xỉu",
    "version": "1.0.0",
    "description": "hỗ trợ người chơi tham gia game",

    "content_scripts": [
        {
          "matches": ["https://xsmb88.com/*"],
          "js": ["./dist/bundle.min.js"]
        }
      ],
    "browser_action": {
    
      "default_title": "Tool Tài Xỉu",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }
  